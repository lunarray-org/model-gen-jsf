<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<name>modelgeneration</name>

	<component>
		<component-type>HtmlGeneratedForm</component-type>
		<component-class>org.lunarray.model.generation.jsf.components.form.HtmlGeneratedForm</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>org.lunarray.model.generation</component-family>
			<renderer-type>HtmlGeneratedFormRenderer</renderer-type>
			<renderer-class>org.lunarray.model.generation.jsf.components.form.HtmlGeneratedFormRenderer</renderer-class>
		</renderer>
	</render-kit>


	<component>
		<component-type>HtmlGeneratedTable</component-type>
		<component-class>org.lunarray.model.generation.jsf.components.table.HtmlGeneratedTable</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>org.lunarray.model.generation</component-family>
			<renderer-type>HtmlGeneratedTableRenderer</renderer-type>
			<renderer-class>org.lunarray.model.generation.jsf.components.table.HtmlGeneratedTableRenderer</renderer-class>
		</renderer>
	</render-kit>

	<component>
		<component-type>HtmlAction</component-type>
		<component-class>org.lunarray.model.generation.jsf.components.shared.action.HtmlAction</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>org.lunarray.model.generation</component-family>
			<renderer-type>HtmlActionRenderer</renderer-type>
			<renderer-class>org.lunarray.model.generation.jsf.components.shared.action.HtmlActionRenderer</renderer-class>
		</renderer>
	</render-kit>

	<component>
		<component-type>HtmlFormat</component-type>
		<component-class>org.lunarray.model.generation.jsf.components.shared.format.HtmlFormat</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>org.lunarray.model.generation</component-family>
			<renderer-type>HtmlFormatRenderer</renderer-type>
			<renderer-class>org.lunarray.model.generation.jsf.components.shared.format.HtmlFormatRenderer</renderer-class>
		</renderer>
	</render-kit>

	<component>
		<component-type>HtmlSelectingCheckbox</component-type>
		<component-class>org.lunarray.model.generation.jsf.components.shared.check.HtmlSelectingCheckbox</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>org.lunarray.model.generation</component-family>
			<renderer-type>HtmlSelectingCheckboxRenderer</renderer-type>
			<renderer-class>org.lunarray.model.generation.jsf.components.shared.check.HtmlSelectingCheckboxRenderer</renderer-class>
		</renderer>
	</render-kit>

	<converter>
		<converter-for-class>java.util.Calendar</converter-for-class>
		<converter-class>org.lunarray.model.generation.jsf.converters.CalendarConverter</converter-class>
	</converter>

	<converter>
		<converter-for-class>java.util.Date</converter-for-class>
		<converter-class>org.lunarray.model.generation.jsf.converters.DateConverter</converter-class>
	</converter>

	<converter>
		<converter-for-class>java.sql.Date</converter-for-class>
		<converter-class>org.lunarray.model.generation.jsf.converters.SqlDateConverter</converter-class>
	</converter>
</faces-config>
