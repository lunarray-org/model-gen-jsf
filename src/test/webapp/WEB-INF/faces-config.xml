<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>formTestPB</managed-bean-name>
		<managed-bean-class>org.lunarray.model.generation.jsf.test.FormTestPB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<navigation-case>
			<from-outcome>reloadForm</from-outcome>
			<to-view-id>/test-form.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>reloadTable</from-outcome>
			<to-view-id>/test-table.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
